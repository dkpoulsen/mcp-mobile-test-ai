# Test Environment Configuration
NODE_ENV=test

# LLM Configuration
LLM_PROVIDER=openai
LLM_API_KEY=sk-test-key-placeholder
LLM_MODEL=gpt-4
LLM_MAX_TOKENS=1000
LLM_TEMPERATURE=0.5

# MCP Configuration
MCP_MAX_RETRIES=1
MCP_TIMEOUT=10000

# Device Configuration
DEVICE_TYPE=both
APPIUM_PORT=4723

# Execution Configuration
MAX_PARALLEL=3
EXECUTION_TIMEOUT=30000
RETRY_COUNT=0

# Logging Configuration
LOG_LEVEL=warn
LOG_FILE=logs/test.log
LOG_FORMAT=text

# Feature Flags
ENABLE_TRACING=false
ENABLE_METRICS=false
