# Development Environment Configuration
NODE_ENV=development

# LLM Configuration
LLM_PROVIDER=openai
LLM_API_KEY=sk-dev-key-placeholder
LLM_MODEL=gpt-4
LLM_MAX_TOKENS=2000
LLM_TEMPERATURE=0.7

# MCP Configuration
MCP_MAX_RETRIES=3
MCP_TIMEOUT=30000

# Device Configuration
DEVICE_TYPE=both
APPIUM_PORT=4723

# Execution Configuration
MAX_PARALLEL=1
EXECUTION_TIMEOUT=60000
RETRY_COUNT=1

# Logging Configuration
LOG_LEVEL=debug
LOG_FILE=logs/mcp-mobile-test.log
LOG_FORMAT=text

# Feature Flags
ENABLE_TRACING=true
ENABLE_METRICS=false
